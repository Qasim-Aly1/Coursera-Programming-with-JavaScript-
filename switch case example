var day = "Thursday";
switch (day){
    case "Monday":
    console.log("university open have to get up on time to prepare for uni");
    break;
    case "Tuesday":
    console.log("university + office manage time ");
    break;    
    case "Wednesday":
    console.log("university + office + assigment very busy day")
    break;
    case "Thursday":
    console.log("university +  Office + Cricket match ");
    break;
    case "Friday":
    console.log("Halfday on university + friday prayer time ");
    break;
    case "Saturday":
    console.log("Uni is off have chill day ");
    break;
    case "Sunday":
    console.log("full chill day ");
    break;
    default:
    console.log("There is no such a day on weeks day")
}
